<?xml version="1.0" encoding="utf-8" ?>
<Rules FriendlyName="InjectionCop">
  <Rule TypeName="TypeParser" Category="InjectionCop.SQLi" CheckId="IC_SQLi">
    <Name>Check for Fragment Violations</Name>
    <Description>Methods that are vulnerable to SQL injection must be called with safe Parameters</Description>
    <Url>injectioncop.codeplex.com</Url>
    <Resolution>
      Make parameter safe
    </Resolution>
    <MessageLevel Certainty="95">Warning</MessageLevel>
    <FixCategories>NonBreaking</FixCategories>
    <Email />
    <Owner />
  </Rule>
</Rules>
